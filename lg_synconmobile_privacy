function gotoLogin(user_id) {
	delete __params.user_display_name;
	delete __params.user_id;
	delete __params.user_id_type;
	delete __params.user_old_pw;
	delete __params.svc_user_no;
	delete __params.itg_pw;
	delete __params.itg_svc_user_no;
	delete __params.itg_status;
	delete __params.itg_user_id_type;
	delete __params.itg_terms;
	delete __params.noticed_terms_data;

	if(!LGMUtil.isNull(user_id)) {
		__params.user_display_name = user_id;
	}
	LGMUtil.formSubmit("/login/signIn", "POST", __params);
}




