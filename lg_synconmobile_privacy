<script type="text/javascript">
/**
 * Error manager
 * LGMM.ERROR
 * LGMM.ERROR.getMessage('code')
 */
LGMM.ERROR=(function(){
	/**
	* HTTP STATUS ERROR method
	* @static
	* @method _getHttpStatusError
	* @param {String} parameter String
	* @return {String} Returns String
	*/
	function _getHttpStatusError(__opt) {
		var _msg = '';
		switch(__opt) {
			case '400' :
				_msg = "400 Bad Request";
			break;
			case '401' :
				_msg = "401 Unauthorized";
			break;
			case '403' :
				_msg = "403 Forbidden";
			break;
			case '405' :
				_msg = "405 Method Not Allowed";
			break;
			case '406' :
				_msg = "406 Not Acceptable";
			break;
			case '411' :
				_msg = "411 Length Required";
			break;
			case '412' :
				_msg = "412 Precondition Failed";
			break;
			case '500' :
				_msg = "500 Internal Server Error";
			break;
		}
		return _msg;
	}

	/**
	* Membership Error method (ERR.xx)
	* @static
	* @method _getMembershipMobileErr
	* @param {String} parameter String
	* @return {String} Returns String
	*/
	var _getMembershipMsg = function (__opt) {
		var _msg = '';

		switch(__opt) {
			case "01_account_07.04" :
				if(__params.country.toUpperCase() === "KR"){
					_msg = "· 8자 이상 : 대문자, 소문자, 숫자, 특수문자 중 3가지 이상 조합"+"<br/>"+"· 10자 이상: 대문자, 소문자, 숫자, 특수문자 중 2가지 이상 조합"+"<br/>"+"(단, 2014년 이전 smart TV 모델에서는 특수문자가 지원되지 않습니다. 대문자, 소문자, 숫자의 조합을 사용해 주세요.)"; break;
				}else{
					_msg = "· 8자 이상 : 대문자, 소문자, 숫자, 특수문자 중 3가지 이상 조합"+ "(단, 2014년 이전 smart TV 모델에서는 특수문자가 지원되지 않습니다. 대문자, 소문자, 숫자의 조합을 사용해 주세요.)"; break; // 영문, 숫자, 특수기호 중 2가지 이상을 조합하여 8자리 이상의 비밀번호를 입력해주세요.
				}
			case "alert.01" : _msg = "비밀번호 재설정이 완료되었습니다."; break; // 비밀번호 재설정이 완료되었습니다.
			case "alert.03" : _msg = "보조 이메일 정보가 없어 비밀번호를 재설정할 수 없습니다."; break; // 보조 이메일 정보가 없어 비밀번호를 재설정할 수 없습니다.
			case "alert.04" : _msg = "서비스 이용을 위해서 신규 회원가입이 필요합니다."; break; // 서비스 이용을 위해서 신규 회원가입이 필요합니다.
			case "alert.05" : _msg = "비밀번호 변경이 완료되었습니다."; break; // 비밀번호 변경이 완료되었습니다.
