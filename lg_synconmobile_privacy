LGMUtil.errDescHtml = function(__msg, __$obj, __$focusObj) {
	if(__msg === "") {
		if(__$obj !== undefined) {
			__$obj.html(__msg).hide();
		}
		return;
	}
	if(__$obj !== undefined) {
		__$obj.html(__msg).show();
	}
	if(__$focusObj !== undefined && __$focusObj.length === 1) {
		setTimeout( function() { __$focusObj.focus(); }, 200);
	}
};



function searchKey(KEY_NAME) {
	var returnData = "";
	$.ajax({
		type  : "POST",
		url   : LGMM.DEFAULT_CONTEXT_PATH + '/searchKey',
		async : false,
		data  : {
			key_name   : KEY_NAME,
			sever_type : LGMM.SERVER_TYPE
		},
		success : function(data) {
			var JSONData = JSON.parse(data);
			
			returnData = JSONData.returnData;
		},
		error : function() { }
	});
	
	
	return returnData;
};

function terms_name_settion(termsType) {
	var terms_name = "";
	if(termsType == "S_GGH" || termsType == "S_ALX" || termsType == "S_CLV" || termsType == "S_YNDX") {
		terms_name = "thinQ";
	}else if(termsType === "S_INIT") {
		terms_name = "INNIT";
	}else if (termsType === "S_DROP") {
		terms_name = "DROP";
	}else if (termsType === "S_SDCF") {
		terms_name = "SIDECHEF";
	}else if (termsType === "S_TVLA") {
		terms_name = "TOVALA";
	}else if (termsType === "S_ALX") {
		terms_name = "ALEXA";
	}else if (termsType === "S_HSEM") {
		terms_name = "HANSSEM";
	}else if (termsType === "S_LGU") {
		terms_name = "LGU";
	}else if (termsType === "S_SKT") {
		terms_name = "SKT";
	}else if (termsType === "S_KTC") {
		terms_name = "KT";
	}else if (termsType === "S_NADR") {
		terms_name = "NADR";
	}else if (termsType === "S_ASUS") {
		terms_name = "ASUS";
	}else if (termsType === "S_TMLL") {
		terms_name = "TMALL";
	}else if (termsType === "S_CCSP") {
		terms_name = "Kakao i";
	}else if (termsType === "S_KKAC") {
		terms_name = "Kakao";
	}else if (termsType === "S_SKNU") {
		terms_name = "SKT NUGU";
	}else if (termsType === "S_KTHD") {
		terms_name = "KTHD";
	}else if (termsType === "S_YNDX") {
		terms_name = "YANDEX";
	}else if (termsType === "S_GGH") {
		terms_name = "Google Home";
	}else if (termsType === "S_CLV") {
		terms_name = "CLOVA";
	}else if (termsType === "S_KTSC") {
		terms_name = "KT";
	}else if (termsType === "S_OFIO") {
		terms_name = "LG 오피스 IoT";
	}else {
		terms_name = "LG Account";
	}

	return terms_name;
};
