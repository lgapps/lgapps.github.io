			case "S006-1.10" : _msg = "한번 가입한 아이디는 변경할 수 없습니다. {%}(으)로 진행하시겠습니까?"; break; // 한번 가입한 아이디는 변경할 수 없습니다. {%}(으)로 진행하시겠습니까?

		}

		return _msg;
	};

	return {
		getHttpStatusError  : _getHttpStatusError,
		getMessage          : _getMembershipMsg
	};
})();
</script>

<script type="text/javascript">
    var _isGdprCountry = false
    ;
	var terms_url_list = [];

	$(".spx-actionbar-button").on("click", function(__e) {
		__e.preventDefault();
		backFunc();
	});
	$(".spx-actionbar-title").on("click", function(__e) {
		__e.preventDefault();
		backFunc();
	});
	$("#btn_next").on("click", function(__e) {
		__e.preventDefault();
		$("html,body").animate({
			scrollTop: $("html,body").prop('scrollHeight')
		}, 800);

		backFunc();
	});

	function backFunc() {
	    if (__params.division.toUpperCase() === "HA:T20") {
	        // thinQ 앱에서 넘어온 경우
            window.NativeInterface.closeView();
        } else if(__params.division.toUpperCase() === "THINQ:REDIRECT" && !LGMUtil.isNull( __params.callback_url )){
			LGMUtil.externSubmit(__params.callback_url, "GET",
					{
						returnCode          : 400,
						returnDescription   : "cancel"
					}
			);
        } else {
	        // google 로그인 등에서 넘어온 경우
            window.open('','_self').close();
        }
    }

    function isGdprCountry() {
        _isGdprCountry  = false;
        if (   __params.country === "AT" || __params.country === "BE" || __params.country === "BG" || __params.country === "HR" || __params.country === "CZ"
            || __params.country === "DK" || __params.country === "EE" || __params.country === "FI" || __params.country === "FR" || __params.country === "DE"
            || __params.country === "GR" || __params.country === "HU" || __params.country === "IS" || __params.country === "IE" || __params.country === "IT"
            || __params.country === "LV" || __params.country === "LT" || __params.country === "LU" || __params.country === "NL" || __params.country === "NO"
            || __params.country === "PL" || __params.country === "PT" || __params.country === "RO" || __params.country === "SK" || __params.country === "SI"
            || __params.country === "ES" || __params.country === "SE" || __params.country === "CH" || __params.country === "GB" || __params.country === "CY"
            || __params.country === "MT" || __params.country === "LI" ){
            _isGdprCountry =  true;
        }
    }

	/**
	 * 상단 title 표시
	 */
	function setTermTitle(title) {
		$(".spx-actionbar-title").append("<h1></h1>");
        $(".spx-actionbar-title h1").text(title);
	}

    /**
     * Region % Language 표시
     */
    function setRegionLanguage() {
        var region;
        var language;

        for (var i = 0; i < countries.length; i++) {
            if (countries[i].country_code === __params.country) {
                region = countries[i].country_name;

                for (var j = 0; j < countries[i].country_lang.length; j++) {
                    if (countries[i].country_lang[j].lang_code === __params.language) {
                        language = countries[i].country_lang[j].lang_name;
                    }
                }
            }
        }

        $('#region_language').text(region + "/" + language);
    }

	function setTermsDetail(url) {
        __params.detail_terms_url = url;

		if(LGMUtil.isNull(__params.detail_terms_url)) {
			return;
		}
		$.ajax({
			url : LGMM.PROXY_HTML,
			type : 'GET',
			dataType : 'html',
			data : {
				target : url
			}
		})
		.done(function (__data) {
			$(".section35_topmessage").html( __data );
		})
		.fail(function (__jqXHR, __textStatus, __errorThrown) {
			$(".section35_topmessage").html("");
		});
	}
	
	$(window).on("api_call_success", function(__e, __data) {
		$('#terms_list').empty();
		var terms = __data.info.terms
			, i, year, month, day, option;

		if (terms.length > 0){
            for(i = 0; i < terms.length; i++){
                year = terms[i].applyStartDate.substring(0,4)+"/";
                month = terms[i].applyStartDate.substring(4,6)+"/";
                day = terms[i].applyStartDate.substring(6,8);
                if(i == 0){
                    option = $("<option id='" + terms[i].termsID +"'>현행 시행일자 "+year+month+day+"</option>");
                }else {
                    option = $("<option id='" + terms[i].termsID +"'>이전 시행일자 "+year+month+day+"</option>");
                }
                $('#terms_list').append(option);
                terms_url_list[i] = terms[i].termUrlList[0].url;
            }
            $("#terms_history").show();
            setTermsDetail(terms[0].termUrlList[0].url);
            setTermTitle(terms[0].termsTypeName);
        }

	});
	
	$(window).on("api_call_fail", function(__e, __data) {
		var _errCode = __data["error"]["code"]
		,_langMsg = __data["error"]["lang_msg"]
		,api_nm = __data["api_nm"];

		alert(_langMsg);
	});
	
	$("#terms_list").on("change", function() {
		var idx = $("#terms_list option").index($("#terms_list option:selected"));
		setTermsDetail(terms_url_list[idx]);
	});

    $('#region_language').on("click", function () {
        LGMUtil.formSubmit("/customer/terms_language_list", "POST", __params);
    });

    $(function() {
        __params.terms_type = "SVC708:PRV_HEALTH";
        __params.svc_terms = "svc";  
        __params.show_countries = "KR,US,CA,JP";  
        __params.hide_countries = "";  
        
        isGdprCountry();

        __params.terms_type = __params.terms_type.replace("_GDPR", "");

        if (__params.call_type != "svc" && (_isGdprCountry || (__params.country == 'KR' && __params.terms_type == 'A_ITG_PRV'))) { // A_ITG_PRV 약관은 한국이 GDPR에 속함
            if (__params.terms_type.search("_GDPR") < 0) {
                // 이미 GDPR 국가에서 넘어온 경우 붙여있음 -> 없을 경우에만 _GDPR 붙임
                __params.terms_type = __params.terms_type + "_GDPR";
            }
        }

        
        if(
        		(__params.terms_type.toUpperCase().startsWith("SVC708") && __params.terms_type.toUpperCase().indexOf("PRV_KR_HEALTH") > -1) 
        		|| (__params.terms_type.toUpperCase().startsWith("SVC708") && __params.terms_type.toUpperCase().indexOf("PRV_HEALTH") > -1) 
  		){
        	if(__params.country == 'KR'){
        		__params.terms_type='SVC708:PRV_KR_HEALTH';
        	}else{
        		__params.terms_type='SVC708:PRV_HEALTH';
        	}
        }
        
        LGMApi.if_002(LGMUtil.converterEscape(__params.terms_type), __params.svc_terms);
        setRegionLanguage();

    }());

</script>
</body>
</html>
